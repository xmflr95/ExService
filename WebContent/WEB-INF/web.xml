<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>ExternalService</display-name>
  <servlet>
    <servlet-name>mainServiceServlet</servlet-name>
    <servlet-class>Servlet.MainService</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>mainServiceServlet</servlet-name>
    <url-pattern>/main</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>crawlingServlet</servlet-name>
    <servlet-class>Servlet.Crawling</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>crawlingServlet</servlet-name>
    <url-pattern>/crawling</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>exportDataServlet</servlet-name>
    <servlet-class>Servlet.ExportData</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>exportDataServlet</servlet-name>
    <url-pattern>/export</url-pattern>
  </servlet-mapping>
</web-app>